    "cypress": "cypress run",
    "cypress:ci": "server-test start:ci http://localhost:3000 cypress",
    "format": "eslint . --fix",
    "dev": "nodemon ."
  },
  "description": "Node.js template for Twilio sample apps",
  "author": {
    "name": "Twilio",
    "email": "open-source@twilio.com"
  },
  "license": "MIT",
